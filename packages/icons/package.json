{
  "name": "marshmallow-icons",
  "version": "0.1.0",
  "description": "Marshmallow UDS SVG icons as React components",
  "license": "SEE LICENSE IN LICENSE",
  "author": "The Don't Stop Brand Inc operating as Marshmallow Studio",
  "repository": {
    "type": "git",
    "url": "https://github.com/ayoelutilo/marshmallow-icons.git"
  },
  "keywords": [
    "icons",
    "react",
    "svg",
    "components",
    "marshmallow",
    "design-system"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "peerDependencies": {
    "react": ">=17"
  },
  "devDependencies": {
    "@svgr/core": "^8.1.0",
    "@types/react": "^18.3.12",
    "react": "^18.3.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "generate": "node ./scripts/generate-icons.mjs",
    "build": "npm run generate && tsup",
    "prepack": "npm run build"
  }
}


